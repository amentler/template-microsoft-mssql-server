[
    {
        "key": "ctrl+shift+e",
        "command": "sqltools.executeQueryFromFile"
    },
    {
        "key": "ctrl+shift+r",
        "command": "sqltools.executeQuery",
        "when": "editorHasSelection && editorTextFocus && !config.sqltools.disableChordKeybindings"
    },
    {
        "key": "ctrl+e ctrl+e",
        "command": "-sqltools.executeQuery",
        "when": "editorHasSelection && editorTextFocus && !config.sqltools.disableChordKeybindings"
    },
    {
        "key": "ctrl+e",
        "command": "sqltools.executeCurrentQuery"
    }
]